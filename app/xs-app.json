{
    "welcomeFile": "/launchpad",
    "authenticationMethod": "none",
    "routes": [
      {
        "source": "^/resources/(.*)$",
        "target": "/resources/$1",
        "destination": "ui5",
        "authenticationType": "none"
      },
      {
        "source": "^/test-resources/(.*)$",
        "target": "/test-resources/$1",
        "destination": "ui5",
        "authenticationType": "none"
      },
      {
        "source": "^/ticketingsystem/(.*)$",
        "target": "/ticketingsystem/$1",
        "localDir": "./ticketingsystem/webapp",
        "authenticationType": "none"
      },
      {
        "source": "^/ticketingform/(.*)$",
        "target": "/ticketingform/$1",
        "localDir": "./ticketingform/webapp",
        "authenticationType": "none"
      },
      {
        "source": "^/launchpad/(.*)$",
        "target": "/$1",
        "service": "html5-apps-repo-rt",
        "authenticationType": "none"
      },
      {
        "source": "^(.*)$",
        "target": "/$1",
        "destination": "srv_api",
        "authenticationType": "xsuaa"
      }
    ]
  }
  