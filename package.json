{
  "name": "cap-project-portfolio",
  "version": "1.0.0",
  "description": "Ticket System MSG.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/sqlite": "^1.7.7",
    "@sap/cds-hana": "^2",
    "@sap/xssec": "^4",
    "express": "^4",
    "passport": "^0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.7.0",
    "@sap/cds": "^8.5.1",
    "@sap/cds-compiler": "^4.0.0",
    "@sap/cds-dk": "^8",
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
    "@sap/hdi-deploy": "^4",
    "@sap/ux-specification": "^1.124.6",
    "@sapui5/ts-types": "^1.131.1",
    "cds-launchpad-plugin": "^2.2.0",
    "eslint": "^9",
    "mbt": "^1.2.7",
    "rimraf": "^6"
  },
  "scripts": {
    "start": "cds-serve",
    "undeploy": "cf undeploy cap-project-portfolio --delete-services --delete-service-keys --delete-service-brokers",
    "build": "mbt build",
    "deploy": "cf deploy mta_archives/cap-project-portfolio_1.0.0.mtar"
  },
  "cds": {
    "requires": {
      "db": {
        "development": {
          "kind": "sqlite",
          "impl": "@cap-js/sqlite",
          "credentials": {
            "url": "db.sqlite"
          }
        },
        "production": {
          "kind": "hana-cloud",
          "impl": "@cap-js/hana"
        }
      },
      "auth": {
        "development": {
          "kind": "dummy"
        },
        "production": {
          "kind": "xsuaa"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "sapux": [
    "app/ticketingsystem"
  ],
  "engines": {
    "node": "^20.11.1",
    "npm": "^10.2.4"
  }
}
